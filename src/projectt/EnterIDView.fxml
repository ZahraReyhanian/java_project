<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="481.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="projectt.EnterIDViewController">
   <stylesheets>
      <URL value="@styles.css" />
   </stylesheets>
   <styleClass>
      <String fx:value="mainFxmlClass" />
      <String fx:value="custom" />
   </styleClass>
   <children>
      <Label alignment="CENTER" layoutX="93.0" layoutY="68.0" prefHeight="67.0" prefWidth="294.0" text="Start Chatting" textFill="WHITE">
         <font>
            <Font name="System Bold Italic" size="32.0" />
         </font>
      </Label>
      <Button layoutX="52.0" layoutY="336.0" mnemonicParsing="false" onAction="#returnMenu" style="-fx-background-radius: 5;" styleClass="cacel" text="return" textFill="WHITE">
